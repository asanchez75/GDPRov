                                         rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                                         rdfs:label "Invalidation"^^xsd:string ;
                                         <http://www.w3.org/ns/prov#category> "qualified"^^xsd:string ;
                                         <http://www.w3.org/ns/prov#component> "entities-activities"^^xsd:string ;
