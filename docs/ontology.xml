        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">links step with data used</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">usesData</rdfs:label>
    </owl:ObjectProperty>
    


