        <owl:inverseOf rdf:resource="http://purl.org/adaptcentre/openscience/ontologies/gdprov#usesData"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">isUsedByStep</rdfs:label>
    </owl:ObjectProperty>
    


