    <owl:ObjectProperty rdf:about="http://purl.org/adaptcentre/openscience/ontologies/gdprov#sharesData">
        <rdfs:domain rdf:resource="http://purl.org/adaptcentre/openscience/ontologies/gdprov#DataSharingStep"/>
